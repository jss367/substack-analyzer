{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Streamlit: E2E Walkthrough",
            "type": "debugpy",
            "request": "launch",
            "module": "streamlit",
            "args": [
                "run",
                "scripts/e2e_walkthrough.py",
                "--server.headless=false",
                "--browser.gatherUsageStats=false"
            ],
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "console": "integratedTerminal",
            "justMyCode": false,
            "subProcess": true
        }
    ]
}
